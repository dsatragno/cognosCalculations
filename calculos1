if (Grupo_Ejecutivo in ('(ACC) Oficiales A'; '(ACC) Oficiales B'; '(ACC) Oficiales C'; '(ACC) Oficiales Jefa'))
    then ('(ACC) Oficiales')
    
else if (Grupo_Ejecutivo = ('(ACC) Nuevos Negocios')) 
    then ('(ACC) Nuevos Negocios')
    
else if (Grupo_Ejecutivo in ('(ACC Privados Jefa)'; '(ACC) Privados A'; '(ACC) Privados C'; '(ACC) Privados D'; '(ACC) Privados E'; '(ACC) Privados F'; '(ACC) Privados G'; 
