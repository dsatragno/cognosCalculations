(test) - Subsegmento OFI=
IF ( COD_TIPO_DOCUMENTO = 'PUB' OR ROL_EJECUTIVO_ASIGNADO_VTIGER = 'Ejecutivo Cuentas Oficiales' OR CATEGORIA_CLIENTE_VTIGER = 'Oficial' ) THEN 
// falta verificar asignación
        ( 'OFICIALES' ) ELSE
                IF ( (PROM_FACT_FIJA_6_MESES + PROM_FACT_DATOS_6_MESES + PROM_FACT_MOVIL_6_MESES) > 100000 ) THEN
                          ( 'ZAFIRO' ) ELSE  
                                IF ( PROM_FACT_FIJA_6_MESES + PROM_FACT_DATOS_6_MESES + PROM_FACT_MOVIL_6_MESES > 20000 )  THEN     
                                         ( 'PRIVADOS' ) ELSE  ('PYMES')

