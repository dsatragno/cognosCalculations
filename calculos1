Grupo Ejecutivo (feb24)=
if (Grupo_Ejecutivo in ('(ACC) Oficiales A'; '(ACC) Oficiales B'; '(ACC) Oficiales C'; '(ACC) Oficiales Jefa'))
    then ('(ACC) Oficiales')
    
else if (Grupo_Ejecutivo = ('(ACC) Nuevos Negocios')) 
    then ('(ACC) Nuevos Negocios')
    
else if (Grupo_Ejecutivo in ('(ACC Privados Jefa)'; '(ACC) Privados A'; '(ACC) Privados C'; '(ACC) Privados D'; '(ACC) Privados E'; '(ACC) Privados F'; '(ACC) Privados G'; 
    '(ACC) Privados I'; '(ACC) Privados J'; '(ACC) Privados K'; '(ACC) Privados VIP'))
    then ('(ACC) Privados')
    
else if (Grupo_Ejecutivo in ('(ACC) Zafiro A';'(ACC) Zafiro B';'(ACC) Zafiro C' ;'(ACC) Zafiro D';'(ACC) Zafiro E';'(ACC) Zafiro Jefe'))
    then ('(ACC) Zafiro')

else if (Grupo_Ejecutivo in ('(ACCI) Artigas'; '(ACCI) Río Negro';'(ACCI) Rivera';'(ACCI) Salto';'(ACCI) Tacuarembó'; '(ACCI) Paysandú'))
    then ('(ACCI) Zona Norte')

else if (Grupo_Ejecutivo in ('(ACCI) San José';'(ACCI) Soriano';'(ACCI) Canelones';'(ACCI) Colonia';'(ACCI) Flores';'(ACCI) Florida';'(ACCI) Durazno'))
    then ('(ACCI) Zona Oeste')

else if (Grupo_Ejecutivo in ('(ACCI) Treinta y Tres';'(ACCI) Rocha';'(ACCI) Maldonado';'(ACCI) Cerro Largo';'(ACCI) Lavalleja';'(ACCI) Oficiales Interior'))
    then ('(ACCI) Zona Este')

else if (Grupo_Ejecutivo = ('(ADV) Agentes de Venta')) 
    then ('(ADV) Agentes de Venta')
    
else if (Grupo_Ejecutivo = ('(ACPY) Atención Pymes')) 
    then ('(ACPY) Atención Pymes')
    
else if (Grupo_Ejecutivo = ('(ACC) DataCenter')) 
    then ('(ACC) DataCenter')
    
else if (Grupo_Ejecutivo = ('(ACPC) Proyectos')) 
    then ('(ACPC) Proyectos')
    
else if (Grupo_Ejecutivo = ('(ACPY) Ventas Pymes')) 
    then ('(ACPY) Ventas Pymes')
    
else if (Grupo_Ejecutivo = ('(ACPY) Atención Pymes')) 
    then ('(ACPY) Atención Pymes')
    
else if (Grupo_Ejecutivo = ('(MVD) Ventas Internacionales')) 
    then ('(MVD) Ventas Internacionales')
    
else ('(sin unidad)')

