Cálculos de Cognos:
-----------------------------------------------------------------------------------------------------------------------------------
Paquete Vigente:
case when FECHA_VENCE_PAQUETE is not null then 
    case when (FECHA_VENCE_PAQUETE < TO_DATE(
             CONCAT(
                 CONCAT(
                     CONCAT(SUBSTR(FECHA_PRODUCCION,1,4), '-'),
                     CONCAT(SUBSTR(FECHA_PRODUCCION,5,2), '-')), 
                     _day(LAST_DAY(TO_DATE(FECHA_PRODUCCION, 'yyyymm')))), 'yyyy-mm-dd')) then ('Vencido') else ('Vigente') end
            else null end

-------------------------------------------------------------------------------------------------------------------------------------
Porcentaje Paquetes Vigentes: (despues de agregación)

count (case when (FECHA_VENCE_PAQUETE > TO_DATE(
                 CONCAT(
                     CONCAT(
                         CONCAT(SUBSTR(FECHA_PRODUCCION,1,4), '-'),
                         CONCAT(SUBSTR(FECHA_PRODUCCION,5,2), '-')), 
                         _day(LAST_DAY(TO_DATE(FECHA_PRODUCCION, 'yyyymm')))), 'yyyy-mm-dd')) then 1 end)
    /

(count (case when (FECHA_VENCE_PAQUETE > TO_DATE(
                 CONCAT(
                     CONCAT(
                         CONCAT(SUBSTR(FECHA_PRODUCCION,1,4), '-'),
                         CONCAT(SUBSTR(FECHA_PRODUCCION,5,2), '-')), 
                         _day(LAST_DAY(TO_DATE(FECHA_PRODUCCION, 'yyyymm')))), 'yyyy-mm-dd')) then 1 end)
        +
        
count (case when (FECHA_VENCE_PAQUETE <= TO_DATE(
                 CONCAT(
                     CONCAT(
                         CONCAT(SUBSTR(FECHA_PRODUCCION,1,4), '-'),
                         CONCAT(SUBSTR(FECHA_PRODUCCION,5,2), '-')), 
                         _day(LAST_DAY(TO_DATE(FECHA_PRODUCCION, 'yyyymm')))), 'yyyy-mm-dd')) then 1 end)  )

-------------------------------------------------------------------------------------------------------------------------------------------
Paquete Meses Restantes:

((_year(FECHA_VENCE_PAQUETE)
-
_year(TO_DATE(
       CONCAT(
             CONCAT(
                   CONCAT(SUBSTR(FECHA_PRODUCCION,1,4), '-'),
                   CONCAT(SUBSTR(FECHA_PRODUCCION,5,2), '-')), 
                   _day(LAST_DAY(TO_DATE(FECHA_PRODUCCION, 'yyyymm')))), 'yyyy-mm-dd'))) * 12) 
+
(_month(FECHA_VENCE_PAQUETE)
-
_month(TO_DATE(
       CONCAT(
             CONCAT(
                   CONCAT(SUBSTR(FECHA_PRODUCCION,1,4), '-'),
                   CONCAT(SUBSTR(FECHA_PRODUCCION,5,2), '-')), 
                   _day(LAST_DAY(TO_DATE(FECHA_PRODUCCION, 'yyyymm')))), 'yyyy-mm-dd')) )

---------------------------------------------------------------------------------------------------------------------------------------------------------

Acumulado Fact Total Mes (2023 - Activos)=
running-total (Fact Total Mes (2023 - Activos))

Fact Total Mes (2023 - Activos)=
total (case when ( ACTIVO_MDM = 'Activo' and FECHA_PRODUCCION starts with '2023' ) THEN Total_Fact_Mes ELSE 0 END)
---------------------------------------------------------------------------------------------------------------------------------------------------------------

Part. de Clientes Emp. en Total Ingresos ANTEL=
(Fact_Total_Mes_2023_Activos / Fact_Total_de_Antel_dato_de_AEE)

----------------------------------------------------------------------------------------------------------------------------------------------------------------------

(dic22) Cant. Clientes ZAFIRO=
COUNT ( DISTINCT (CASE WHEN ( FECHA_PRODUCCION = '202212' AND Subsegmento = 'ZAFIRO' and ACTIVO_MDM = 'Activo') THEN PARTY_ID ELSE NULL END) for report )


(dic22) Cant. Clientes Priv. con Fact. mayor a $ 20.000=
COUNT ( DISTINCT (CASE WHEN ( FECHA_PRODUCCION = '202212' AND Subsegmento in ('PRIVADOS';'ZAFIRO') and ACTIVO_MDM = 'Activo') THEN PARTY_ID ELSE NULL END) for report )

Diferencia de Fact. Total (dic22 a dic23)=
case when ( ( FECHA_PRODUCCION = '202312') ) then Total_Fact_1 else 0 end -
case when ( ( FECHA_PRODUCCION = '202212' ) ) then Total_Fact_1 else 0 end 


Porc. Fact. Pymes Mes en Total Corp. Mes=
total ((case when (Subsegmento = 'PYMES') then Total_Fact_1 end)) 
/ total (Total_Fact_1)


Fact. Pymes Mes=
total ((case when (Subsegmento = 'PYMES') then Total_Fact_1 end)) 


Variación Cant. Clientes Pymes (a dic22)=
( Cant_Clientes_Pymes / i_DIC_22_Cant_Clientes_Pymes ) - 1


Cant_Clientes_Pymes=
count (case when ((Subsegmento = 'PYMES')) then 1 else 0 end)

Variación Cant. Clientes Zafiro (a dic22)=
( Cant_Clientes_Zafiro / i_DIC_22_Cant_clientes_Zafiro ) - 1

Cant. Clientes Zafiro=
count (case when ((Subsegmento = 'ZAFIRO')) then 1 end)

Cant clientes privados fact > 20k=
count (case when ((Total_Fact > 20000) and not (Subsegmento = 'OFICIALES')) then 1 end)

Zona (Mvd/Int)=
if (Zona_Cliente in ('ZONA NORTE';'ZONA ESTE';'ZONA OESTE')) then ('INTERIOR') else ('MONTEVIDEO')


Facturación Acumulada 2023=
(case when FECHA_PRODUCCION starts with '2023' then (Total_Fact) else 0 end) 


(DIC-22) Cant. clientes Zafiro=
370

Variación Cant. Clientes Priv. Fact 20k (a dic22)=
( Cant_clientes_privados_fact_20k / i_DIC_22_Cant_clientes_privados_con_fact_20K) - 1


(DIC-22) Cant. clientes privados con fact. >20K=
2365

Zona Cliente=
if (DEPARTAMENTO_CLIENTE in (
'MALDONADO';'ROCHA';'TREINTA Y TRES';'CERRO LARGO';'LAVALLEJA') )
   then ('ZONA ESTE')
   else if (DEPARTAMENTO_CLIENTE in ('CANELONES';'SAN JOSE';'SAN JOSÉ';'FLORES';'FLORIDA';'COLONIA';'DURAZNO';'SORIANO'   ) )
       then ('ZONA OESTE')
       
   else if (DEPARTAMENTO_CLIENTE in ('ARTIGAS'; 'SALTO'; 'PAYSANDÚ';'PAYSANDU';'RIO NEGRO'; 'RÍO NEGRO';'TACUAREMBO'; 'TACUAREMBÓ'; 'RIVERA') )
       then ('ZONA NORTE')
       
   else if (DEPARTAMENTO_CLIENTE in ('MONTEVIDEO') )
       then ('ZONA MONTEVIDEO')       

      else ('(sin dato)')


Asignación=
if  (FECHA_PRODUCCION = ('202310') and EJECUTIVO_ASIGNADO_VTIGER = ('@empresas') )
    then ('Carolina Ortega Yamgochian') 
    else if (EJECUTIVO_ASIGNADO_VTIGER in ('administrador'; 'null null') )
        then ('(No asignados)') 
    else (EJECUTIVO_ASIGNADO_VTIGER)

Total Fact Mes=
(TOTAL_FACT_DATOS + TOTAL_FACT_FIJA + TOTAL_FACT_MOVIL)

ARPU Servicios=
( TOTAL_FACT_FIJA + TOTAL_FACT_DATOS + TOTAL_FACT_MOVIL ) / ( CANT_SERVICIOS_FIJA + CANT_SERVICIOS_DATOS + CANT_SERVICIOS_MOVIL )


(M) Variación fact total respecto a dic22 (pendient)=
(case when FECHA_PRODUCCION = '202312' then (Total_Fact) else 0 end) / 
(case when FECHA_PRODUCCION = '202212' then (Total_Fact) else 0 end)



Fact > 100k = 
case when (PROM_FACT_FIJA_6_MESES + PROM_FACT_DATOS_6_MESES + PROM_FACT_MOVIL_6_MESES) > 100000 then 'Si' else 'No' end

Clientes con fact. mensual mayor a $ 100.000 =
case when ( Total_Fact_Mes >= 100000 ) then ('Fact. mayor a $ 100.000') else ('Fact. hasta $ 99.999') end



Prom Fact Total 6 Meses =
(PROM_FACT_FIJA_6_MESES + PROM_FACT_DATOS_6_MESES + PROM_FACT_MOVIL_6_MESES)



Subsegmento = 
IF ( COD_TIPO_DOCUMENTO = 'PUB' OR ROL_EJECUTIVO_ASIGNADO_VTIGER = 'Ejecutivo Cuentas Oficiales' ) THEN 
// falta verificar asignación
        ( 'OFICIALES' ) ELSE
                IF ( (PROM_FACT_FIJA_6_MESES + PROM_FACT_DATOS_6_MESES + PROM_FACT_MOVIL_6_MESES) > 100000 ) THEN
                          ( 'ZAFIRO' ) ELSE  
                                IF ( PROM_FACT_FIJA_6_MESES + PROM_FACT_DATOS_6_MESES + PROM_FACT_MOVIL_6_MESES > 20000 )  THEN     
                                         ( 'PRIVADOS' ) ELSE  ('PYMES')



Tienen Móvil=
(count( (case when CANT_SERVICIOS_MOVIL > 0 then 1 end) )  
/
count( (case when CANT_SERVICIOS_MOVIL >= 0  then 1 end)))


Tienen Datos=
(count( (case when CANT_SERVICIOS_DATOS > 0 then 1 end))
/
count( (case when CANT_SERVICIOS_DATOS >= 0  then 1 end)))


Tienen Fija=
(count( (case when CANT_SERVICIOS_FIJA > 0 then 1 end))
/
count( (case when CANT_SERVICIOS_FIJA >= 0  then 1 end)))


Fact Total Mensual (prom. 6m)=
(PROM_FACT_FIJA_6_MESES + PROM_FACT_DATOS_6_MESES + PROM_FACT_MOVIL_6_MESES )


ARPU Clientes=
Total_Fact / count( (case when PARTY_ID > 0 then 1 end))


Fact Neta=
Total_Fact - (SALDO_ADEUDADO_FACM + FINANCIACION_ESPECIAL_FACM + EQUIPOS_ACCESORIOS_FACM)


Porc clientes priv fact > 20k con móvil=
count( (case when ((CANT_SERVICIOS_MOVIL > 0) and (Total_Fact > 20000) and not ( Subsegmento = 'OFICIALES'))  then 1 end))  
/
count( (case when ((CANT_SERVICIOS_MOVIL >= 0)  and (Total_Fact > 20000) and not ( Subsegmento = 'OFICIALES') ) then 1 end))


Cant clientes privados fact > 20k=
count (case when ((Total_Fact > 20000) and not (Subsegmento = 'OFICIALES')) then 1 end)


(M) Variación clientes priv fact > 20k=
(count( (case when ((Total_Fact > 20000) and not ( Subsegmento = 'OFICIALES')
    and (FECHA_PRODUCCION = '202309'))  then 1 end)) 
/
count( (case when ((Total_Fact > 20000) and not ( Subsegmento = 'OFICIALES')
    and (FECHA_PRODUCCION = '202212')) then 1 end))) -1


(M) Mant Cant clientes Zafiro a respecto a dic22=
(count (case when ((Subsegmento = 'ZAFIRO') and (FECHA_PRODUCCION = '202309') )  then 1 end) /
count (case when ((Subsegmento = 'ZAFIRO') and (FECHA_PRODUCCION = '202212') ) then 1 end)) -1


(M) Variación fact total respecto a dic22 (pendient)=
(case when FECHA_PRODUCCION = '202309' then (Total_Fact) else 0 end) / 
(case when FECHA_PRODUCCION = '202212' then (Total_Fact) else 0 end)


(M) Mant Cant clientes Zafiro a respecto a dic22
(count (case when ((Subsegmento = 'ZAFIRO') and (FECHA_PRODUCCION = '202312') )  then 1 end) 
/
count (case when ((Subsegmento = 'ZAFIRO') and (FECHA_PRODUCCION = '202212') ) then 1 end)) -1


(M) Variación clientes priv fact > 20k=
(count( (case when ((Total_Fact > 20000) and not ( Subsegmento = 'OFICIALES')
    and (FECHA_PRODUCCION = '202312'))  then 1 end)) 
/
count( (case when ((Total_Fact > 20000) and not ( Subsegmento = 'OFICIALES')
    and (FECHA_PRODUCCION = '202212')) then 1 end))) -1

Porc clientes priv fact > 20k con móvil=
count( (case when ((CANT_SERVICIOS_MOVIL > 0) and (Total_Fact > 20000) and not ( Subsegmento = 'OFICIALES'))  then 1 end))  
/
count( (case when ((CANT_SERVICIOS_MOVIL >= 0)  and (Total_Fact > 20000) and not ( Subsegmento = 'OFICIALES') ) then 1 end))





Facturación total ANTEL=
if (FECHA_PRODUCCION = '202208') then (3398508673) else
if (FECHA_PRODUCCION = '202209') then (3407029598) else
