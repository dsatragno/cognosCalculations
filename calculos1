if (FECHA_PRODUCCION = '202308') then (3634701036) else
if (FECHA_PRODUCCION = '202309') then (3576358674) else (0)


Porc fact corporativos en total Antel=
Total_Fact / Facturación_total_ANTEL


Fact Total Mes=
(TOTAL_FACT_FIJA + TOTAL_FACT_DATOS + TOTAL_FACT_MOVIL)


Zona=
if (DEPARTAMENTO_CLIENTE in ('MALDONADO'; 'ROCHA'; 'TREINTA Y TRES'; 'CERRO LARGO'; 'LAVALLEJA'))
    then ('ESTE')
    
    else if (DEPARTAMENTO_CLIENTE in ('CANELONES'; 'SAN JOSE'; 'FLORES'; 'FLORIDA'; 'COLONIA'; 'DURAZNO'; 'SORIANO'))
    then ('OESTE')
    

    else if (DEPARTAMENTO_CLIENTE in ('ARTIGAS'; 'SALTO'; 'PAYSANDU'; 'RIO NEGRO'; 'TACUAREMBO'; 'RIVERA' ))
    then ('NORTE')
    
    else if (DEPARTAMENTO_CLIENTE in ('MONTEVIDEO'))
    then ('MONTEVIDEO')
    
    else ('(sin dato)')

ARPU Servicios=
( TOTAL_FACT_FIJA + TOTAL_FACT_DATOS + TOTAL_FACT_MOVIL ) / 
( CANT_SERVICIOS_FIJA + CANT_SERVICIOS_DATOS + CANT_SERVICIOS_MOVIL )


ARPU Móvil=
(TOTAL_FACT_MOVIL / CANT_SERVICIOS_MOVIL)


ARPU Datos=
(TOTAL_FACT_DATOS / CANT_SERVICIOS_DATOS)


ARPU Fija=
(TOTAL_FACT_FIJA / CANT_SERVICIOS_FIJA)


